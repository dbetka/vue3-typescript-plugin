{
  "name": "@dbetka/vue3-typescript-plugin",
  "version": "0.1.0",
  "description": "Template for plugin written in vue3 and typescript",
  "license": "MIT",
  "keywords": [
    "vue3",
    "vue",
    "plugin",
    "library",
    "typescript",
    "ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dbetka/vue3-typescript-plugin"
  },
  "author": "dbetka",
  "scripts": {
    "dev": "vite",
    "build": "npm run rollup && npm run build:dts",
    "build:dts": "api-extractor run --local --verbose && tail -n +2 src/globalExtensions.ts >> dist/vue3-typescript-plugin.d.ts",
    "rollup": "rimraf ./dist && rollup -c rollup.config.js",
    "lint": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.scss",
    "lint:all": "npm lint && npm lint:css",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npx stylelint src/**/*.scss --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "eslint src/**/*.vue --fix",
      "git add"
    ]
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.2",
    "chalk": "^4.1.2",
    "eslint": "^7.29.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "rollup": "^2.52.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vue": "^3.0.11",
    "vue-float-menu": "^1.9.0"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "vue": "^3.0.4"
  },
  "main": "dist/vue3-typescript-plugin.js",
  "module": "dist/vue3-typescript-plugin.es.js",
  "umd": "dist/vue3-typescript-plugin.umd.js",
  "types": "dist/vue3-typescript-plugin.d.ts",
  "files": [
    "dist"
  ]
}
